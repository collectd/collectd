	SAVE_CPPFLAGS="$CPPFLAGS"
	SAVE_LDFLAGS="$LDFLAGS"
	CPPFLAGS="$CPPFLAGS $with_curl_cflags"
	LDFLAGS="$LDFLAGS $with_curl_libs"
