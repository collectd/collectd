  cat >>confdefs.h <<_ACEOF
#define HAVE_MODBUS_H 1
_ACEOF

else
  with_libmodbus="no (modbus.h not found)"
fi

done


	CPPFLAGS="$SAVE_CPPFLAGS"
fi
